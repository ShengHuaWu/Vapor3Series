#set("content") {
    <table class="table table-bordered table-hover">
        <thead class="thead-light">
            <tr>
                <th>Name</th>
                <th>Age</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th>#(pet.name)</th>
                <th>#(pet.age)</th>
            </tr>
        </tbody>
    </table>

    <h2>Owned by #(user.name).</h2>
}

#embed("base")
